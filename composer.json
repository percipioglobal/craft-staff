{
    "name": "percipiolondon/craft-staff",
    "description": "Craft Staff Management provides an HR solution for payroll and benefits",
    "type": "craft-plugin",
    "version": "0.2.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-staff"
    ],
    "support": {
        "docs": "https://github.com/Craft Staff Management/craft-staff/blob/master/README.md",
        "issues": "https://github.com/Craft Staff Management/craft-staff/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Percipio",
            "homepage": "http://percipio.london"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.6.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "percipiolondon\\craftstaff\\": "src/"
        }
    },
    "extra": {
        "name": "Staff Management",
        "handle": "staff-management",
        "developer": "Percipio",
        "developerUrl": "http://percipio.london",
        "documentationUrl": "https://github.com/percipioglobal/craft-staff/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/percipioglobal/craft-staff/master/CHANGELOG.md",
        "components": {
            "employers": "percipiolondon\\craftstaff\\services\\Employers",
            "employees": "percipiolondon\\craftstaff\\services\\Employees",
            "payRun": "percipiolondon\\craftstaff\\services\\PayRun",
            "payRunEntries": "percipiolondon\\craftstaff\\services\\PayRunEntries",
            "hardingUsers": "percipiolondon\\craftstaff\\services\\HardingUsers"
        },
        "class": "percipiolondon\\craftstaff\\Craftstaff"
    }
}
